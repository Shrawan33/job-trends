{
    "name": "perception/html",
    "description": "Extended Checkbox & Radio with Icheck bootstrap 4",
    "authors": [
        {
            "name": "Digant Shah",
            "email": "digant@perceptionsystem.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/session": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/view": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "illuminate/database": "^6.0|^7.0|^8.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~8.5"
    },
    "autoload": {
        "psr-4": {
            "Perception\\Libraries\\Html\\Builder\\": "html/builder/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        },
        "laravel": {
            "providers": [
                "Perception\\Libraries\\Html\\Builder\\HtmlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
